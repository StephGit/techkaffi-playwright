#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sgirod/repos/techkaffi-playwright/techkaffi-playwright/node_modules/.pnpm/katex@0.16.22/node_modules/katex/node_modules:/home/sgirod/repos/techkaffi-playwright/techkaffi-playwright/node_modules/.pnpm/katex@0.16.22/node_modules:/home/sgirod/repos/techkaffi-playwright/techkaffi-playwright/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sgirod/repos/techkaffi-playwright/techkaffi-playwright/node_modules/.pnpm/katex@0.16.22/node_modules/katex/node_modules:/home/sgirod/repos/techkaffi-playwright/techkaffi-playwright/node_modules/.pnpm/katex@0.16.22/node_modules:/home/sgirod/repos/techkaffi-playwright/techkaffi-playwright/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../katex/cli.js" "$@"
else
  exec node  "$basedir/../katex/cli.js" "$@"
fi
