#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sgirod/repos/techkaffi-playwright/node_modules/.pnpm/@slidev+cli@51.6.0_@babel+parser@7.27.2_@nuxt+kit@3.17.2_@types+markdown-it@14.1.2_@typ_2747a6a202d7aef0824f0295e093fa0c/node_modules/@slidev/cli/bin/node_modules:/home/sgirod/repos/techkaffi-playwright/node_modules/.pnpm/@slidev+cli@51.6.0_@babel+parser@7.27.2_@nuxt+kit@3.17.2_@types+markdown-it@14.1.2_@typ_2747a6a202d7aef0824f0295e093fa0c/node_modules/@slidev/cli/node_modules:/home/sgirod/repos/techkaffi-playwright/node_modules/.pnpm/@slidev+cli@51.6.0_@babel+parser@7.27.2_@nuxt+kit@3.17.2_@types+markdown-it@14.1.2_@typ_2747a6a202d7aef0824f0295e093fa0c/node_modules/@slidev/node_modules:/home/sgirod/repos/techkaffi-playwright/node_modules/.pnpm/@slidev+cli@51.6.0_@babel+parser@7.27.2_@nuxt+kit@3.17.2_@types+markdown-it@14.1.2_@typ_2747a6a202d7aef0824f0295e093fa0c/node_modules:/home/sgirod/repos/techkaffi-playwright/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sgirod/repos/techkaffi-playwright/node_modules/.pnpm/@slidev+cli@51.6.0_@babel+parser@7.27.2_@nuxt+kit@3.17.2_@types+markdown-it@14.1.2_@typ_2747a6a202d7aef0824f0295e093fa0c/node_modules/@slidev/cli/bin/node_modules:/home/sgirod/repos/techkaffi-playwright/node_modules/.pnpm/@slidev+cli@51.6.0_@babel+parser@7.27.2_@nuxt+kit@3.17.2_@types+markdown-it@14.1.2_@typ_2747a6a202d7aef0824f0295e093fa0c/node_modules/@slidev/cli/node_modules:/home/sgirod/repos/techkaffi-playwright/node_modules/.pnpm/@slidev+cli@51.6.0_@babel+parser@7.27.2_@nuxt+kit@3.17.2_@types+markdown-it@14.1.2_@typ_2747a6a202d7aef0824f0295e093fa0c/node_modules/@slidev/node_modules:/home/sgirod/repos/techkaffi-playwright/node_modules/.pnpm/@slidev+cli@51.6.0_@babel+parser@7.27.2_@nuxt+kit@3.17.2_@types+markdown-it@14.1.2_@typ_2747a6a202d7aef0824f0295e093fa0c/node_modules:/home/sgirod/repos/techkaffi-playwright/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@slidev/cli/bin/slidev.mjs" "$@"
else
  exec node  "$basedir/../@slidev/cli/bin/slidev.mjs" "$@"
fi
